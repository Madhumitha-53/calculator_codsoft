#include <iostream>
using namespace std;
int main(){
int a, b;
char operation;
cout<<"enter a: ";
cin>>a;
cout<<"enter b: ";
cin>>b;
cout<<"enter operation: ";
cin>>operation;
switch(operation){

case '+':
cout<<"result: "<<a+b<<endl;
break;

case '-':
cout<<"result: "<<a-b<<ebdl;
break;

case '*':
cout<<"result: "<<a*b<<endl;
break;

case '/':
cout<<"result: "<<a/b<<endl;
break;

default:
cout<<"invalid"<<endl;
}
return 0;
}
